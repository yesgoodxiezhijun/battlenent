\" :src=\"item.img_url\">\r\n\t\t\t\t\t\t<div class=\"mui-media-body\">{{item.title}} \r\n\t\t\t\t\t\t<p class='mui-ellipsis '>\r\n                        <span>{{item.ctime |datetimeFilter}}</span>\r\n                         <span>点击{{item.point}}次</span>\r\n                        </p>\r\n\t\t\t\t\t\t</div>\r\n\t\t\t\t\t</router-link >\r\n\t\t\t\t</li>\r\n   </ul>    \r\n   <!--加载跟多-->\r\n   <mt-button type=\"primary\"size=\"large\"@click=\"getMore\">加载跟多....</mt-button>          \r\n</div>\r\n</template>\r\n<script>\r\n    export default{\r\n         data(){ return {\r\n              list:[] ,\r\n              pno:1,  //当前页码\r\n              pageSize:7 //页大小\r\n              } },\r\n         methods:{\r\n             //加载下一页的数据\r\n             getMore(){\r\n                 //1:修改当前页码+1\r\n                 this.pno++;\r\n                 //2：发送请求get\r\n                 var url=\"http://127.0.0.1:3000\";\r\n                 url+='/newslist?pno='+this.pno;\r\n                 url+='&pageSize='+this.pageSize;\r\n                 //3：获取下一页数据保存list\r\n                 this.axios.get(url).then(result=>{\r\n                     //3.1拼接二个数组\r\n                     var rows=this.list.concat(result.data.data);\r\n                     //3.2赋值list\r\n                     this.list=rows;\r\n                     //this.list=result.data.data;\r\n                 })\r\n\r\n             },\r\n             getnewslist(){\r\n                 var url=\"http://127.0.0.1:3000/newslist\";\r\n             this.axios.get(url).then(result=>{\r\n                 this.list=result.data.data;\r\n                 }) \r\n             }\r\n         },\r\n         created(){\r\n             console.log(this.$route.query.nid);\r\n             this.getnewslist();\r\n         }\r\n    }\r\n</script>\r\n<style l>\r\n    .app-newslist li .mui-ellipsis{\r\n        display:flex;\r\n        font-size:12px;\r\n        color:#226aff;\r\n        justify-content:space-between;\r\n    }\r\n</style>"]}]}